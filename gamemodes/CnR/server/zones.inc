FUNCTION OnPlayerEnterZone(playerid,areaid)
{
	new string[MAX_ZONE_NAME + 6];
	if(PlayerInfo[playerid][pClassSelection])
	{
		format(PlayerInfo[playerid][pZoneName],MAX_ZONE_NAME,"Class Selection");
		format(string,sizeof(string),"~y~Class Selection");
		PlayerTextDrawSetString(playerid,ZoneNameTD,string);
		return 1;
	}
	if(PlayerInfo[playerid][pInterior] != 0) return 1;
	if(!PlayerInfo[playerid][pSpawned]) return 1;
	for(new i=0; i<MAX_POLICE_ZONES; i++)
	{
		if(PoliceZones[i] == areaid)
		{
			PlayerInfo[playerid][pInPoliceProperty]=true;
			break;
		}
	}
	new zone;
	for(new i=sizeof(zones)-1; i>-1; i--)
	{
		if(IsPlayerInDynamicArea(playerid,DynamicZones[i])) 
		{
			zone=i;
			break;
		}
	}
	new bool:police;
	for(new i=0; i<MAX_POLICE_ZONES; i++)
	{
		if(PoliceZones[i] == DynamicZones[zone])
		{
			police=true;
			break;
		}
	}
	format(PlayerInfo[playerid][pZoneName],MAX_ZONE_NAME,"%s",zones[zone][zone_name]);
	if(police) format(string,sizeof(string),"~b~~h~%s",PlayerInfo[playerid][pZoneName]);
	else format(string,sizeof(string),"~w~%s",PlayerInfo[playerid][pZoneName]);
	PlayerTextDrawSetString(playerid,ZoneNameTD,string);
	PlayerInfo[playerid][pZone]=zone;
	return 1;
}

