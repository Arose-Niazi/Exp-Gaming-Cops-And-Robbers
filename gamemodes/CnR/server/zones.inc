FUNCTION SetupZones()
{
	new police_zones;
	//for(new i=0; i<sizeof(zones); i++)printf("%d : %s", i, zones[i][zone_name]);
	for(new i=0; i<MAX_ZONES; i++)
	{
		switch(i)
		{
			case 452 .. 465: 
			{
				if(i == 452) DynamicZones[i]=PoliceZones[police_zones]=CreateDynamicPolygon(zones_ls_police_depart);
				else if(i == 453 || i == 465) DynamicZones[i]=PoliceZones[police_zones]=CreateDynamicCuboid(zones[i][zone_minx],zones[i][zone_miny],zones[i][zone_minz], zones[i][zone_maxx],  zones[i][zone_maxy],zones[i][zone_maxz]);
				else if(i == 463) DynamicZones[i]=PoliceZones[police_zones]=CreateDynamicPolygon(zones_easter_basin);
				else
				{
					DynamicZones[i]=PoliceZones[police_zones]=CreateDynamicRectangle(zones[i][zone_minx],zones[i][zone_miny], zones[i][zone_maxx],  zones[i][zone_maxy]);
				}
				police_zones++;
			}
			case 178: DynamicZones[i]=CreateDynamicPolygon(zones_samp_office_tower);
			case 177: DynamicZones[i]=CreateDynamicPolygon(zones_Yacht_Harbor);
			case 437: DynamicZones[i]=CreateDynamicPolygon(zones_dry_lake);
			default: DynamicZones[i]=CreateDynamicRectangle(zones[i][zone_minx],zones[i][zone_miny], zones[i][zone_maxx],  zones[i][zone_maxy]);
		}
	}
	return 1;
}

FUNCTION DestroyZones()
{
	for(new i=0; i<MAX_ZONES; i++)
	{
		DestroyDynamicArea(DynamicZones[i]);
	}
	return 1;
}

